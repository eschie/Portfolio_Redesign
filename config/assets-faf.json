{
  "main": {
    "vendorJs": {
        "dest": "app/assets/dist/js/lib.js",
        "src": [
            "bower_components/jquery/dist/jquery.min.js",
            "bower_components/modernizr/modernizr.js",
            "bower_components/jquery-waypoints/waypoints.min.js",
            "bower_components/fastclick/lib/fastclick.js"
        ]
    },
    "publicCss": {
        "dest": "app/assets/dist/css/min/main.min.css",
        "src": "app/assets/src/css/main.css"
    },
    "publicJs": {
      "dest": "app/assets/dist/js/app.js",
      "src": [
        "app/assets/src/js/**/*.js"
      ]
    }
  }
}